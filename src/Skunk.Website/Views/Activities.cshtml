@inherits Umbraco.Web.Mvc.UmbracoViewPage
@{
    Layout = "Shared/_Layout.cshtml";
}
<div class="content">
    <h1>@Model.GetPropertyValue("title")</h1>
    <br />
    @Html.Raw(Model.GetPropertyValue("content"))
    <br />
    @Html.Partial("ActivityCalendar")
</div>

@section feed
{
    <a href='@Url.Action("GetActivitySyndicationFeed", "UtilitySurface")'>
        <img src="/img/feed-icon-14x14.png" />
    </a>
}