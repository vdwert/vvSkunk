@inherits Umbraco.Web.Mvc.UmbracoViewPage<Skunk.Website.Entities.Feed.PositionCollection>

<table class="table table-striped">
    @if (Model.Count > 0)
    {
        foreach (Skunk.Website.Entities.Feed.Position position in Model)
        {
            <tr>
                <td>@position.Description</td>
            </tr>
        }
    }
    else
    {
        <tr>
            <td>
                <h5>De stand is nog niet bekend.</h5>
            </td>
        </tr>
    }
</table>

